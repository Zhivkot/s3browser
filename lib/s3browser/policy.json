{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "example-statement-ID",
      "Effect": "Allow",
      "Principal": { "AWS": "*" },
      "Action": [
        "SQS:SendMessage"
      ],
      "Resource": "SQS-queue-ARN",
      "Condition": {
        "ArnLike": {          
          "aws:SourceArn": "arn:aws:s3:*:*:bucket-name"    
        }
      }
    }
  ]
}
